  "main": "src/index.ts",
  "scripts": {
    "eas-build-pre-install": "npm install -g pnpm@10.16.1",
    "eas:ios": "eas build -p ios -e development",
    "dev:ios": "pnpm exec expo run:ios",
    "setup:ios": "pnpm exec expo prebuild --platform ios --clean && cd ios && pod install && cd ..",
    "eas:android": "eas build -p android -e development",
    "dev:android": "pnpm exec expo run:android",
    "setup:android": "pnpm exec expo prebuild --platform android --clean",
    "dev": "pnpm exec expo start",
    "clean": "rm -rf ios android node_modules/.cache"
  },
