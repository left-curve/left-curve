    "accounts": {
      "otherAccounts": "Otras cuentas",
      "addAccount": "Agregar cuenta"
    }
  },
  "accountCreation": {
    "title": "Crear una nueva cuenta",
    "stepper": {
      "title": [
        {
          "declarations": [
            "input account stepper description",
            "input step"
          ],
          "selectors": [
            "step"
          ],
          "match": {
            "step=0": "Tipo de cuenta",
            "step=1": "Depósito"
          }
        }
      ],
      "description": [
        {
          "declarations": [
            "input account stepper description",
            "input step"
          ],
          "selectors": [
            "step"
          ],
          "match": {
            "step=0": "Selecciona tu tipo de cuenta para interactuar dentro de todo el ecosistema",
            "step=1": "Financia tu primera cuenta spot, podrás crear otra cuenta más adelante."
          }
        }
      ]
    },
    "accountType": {
      "title": [
        {
          "declarations": [
            "input account type title",
            "input accountType"
          ],
          "selectors": [
            "accountType"
          ],
          "match": {
            "accountType=spot": "Cuenta Spot",
            "accountType=margin": "Cuenta de margen"
          }
        }
      ],
      "description": [
        {
          "declarations": [
            "input account type description",
            "input accountType"
          ],
          "selectors": [
            "accountType"
          ],
          "match": {
            "accountType=spot": "Puede poseer cualquier activo y participar en cualquier actividad; el costo del gas es el más bajo; solo puede tomar préstamos con sobregarantía.",
            "accountType=margin": "Puede tomar préstamos sin garantía suficiente con un apalancamiento de hasta 5x; solo puede mantener activos incluidos en la lista blanca y participar en actividades incluidas en la lista blanca."
          }
        }
      ]
    }
  },
  "login": {
    "placeholder": "Ingresa tu ",
    "noAccount": "¿No tienes una cuenta?",
    "errors": {
      "usernameRequired": "El nombre de usuario es obligatorio",
      "usernameNotExist": "El nombre de usuario no existe"
    },
