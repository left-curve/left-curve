      "steps": [
        {
          "declarations": [
            "input step"
          ],
          "selectors": [
            "step"
          ],
          "match": {
            "step=0": "Credencial",
            "step=1": "Nombre de usuario"
          }
        }
      ],
      "title": [
        {
          "declarations": [
            "input step"
          ],
          "selectors": [
            "step"
          ],
          "match": {
            "step=0": "Crea tu cuenta",
            "step=1": "Seleccione un nombre de usuario"
          }
        }
      ],
      "description": [
        {
          "declarations": [
            "input step"
          ],
          "selectors": [
            "step"
          ],
          "match": {
            "step=0": "Elija una credencial de inicio de sesión. Puede agregar o eliminar credenciales posteriormente.",
            "step=1": "Su nombre de usuario será público en la cadena y no podrá cambiarse posteriormente."
          }
        }
      ]
    },
    "errors": {
      "usernameTaken": "El nombre de usuario ya está en uso",
      "failedSendingFunds": "Error al enviar fondos",
      "creatingAccount": "Error al crear la cuenta"
    }
  },
  "welcome": {
    "title": "Bienvenido a Dango",
    "description": "Estamos encantados de tenerte con nosotros y esperamos ver qué haces a continuación."
  }
