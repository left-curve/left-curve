      {
        "declarations": [
          "input action"
        ],
        "selectors": [
          "action"
        ],
        "match": {
          "action=signin": "Iniciar sesión con billetera",
          "action=signup": "Regístrate con billetera"
        }
      }
    ],
    "signWithPasskey": [
      {
        "declarations": [
          "input action"
        ],
        "selectors": [
          "action"
        ],
        "match": {
          "action=signin": "Iniciar sesión con clave de acceso",
          "action=signup": "Regístrate con Passkey"
        }
      }
    ],
    "cancel": "Cancelar",
    "delete": "Borrar"
  },
  "notifications": {
    "title": "Notificaciones",
