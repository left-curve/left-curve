    "createAccount": "Create Account",
    "doThisLater": "Do this later",
    "alreadyHaveAccount": "Already have an account?",
    "carousel": {
      "title": [
        {
          "declarations": ["input step"],
          "selectors": ["step"],
          "match": {
            "step=0": "Trade",
            "step=1": "Leverage up",
            "step=2": "Earn"
