      "title": [
        {
          "declarations": ["input account type title", "input accountType"],
          "selectors": ["accountType"],
          "match": {
            "accountType=spot": "Spot Account",
            "accountType=margin": "Margin Account"
          }
        }
      ],
      "description": [
        {
          "declarations": ["input account type description", "input accountType"],
          "selectors": ["accountType"],
          "match": {
            "accountType=spot": "Can hold any asset and partake in any activity; cheapest gas cost; can only take over-collateralized loans.",
            "accountType=margin": "Can take under-collateralized loans up to 5x leverage; can only hold whitelisted assets and partake in whitelisted activities."
          }
        }
      ]
    }
