      "description": [
        {
          "declarations": ["input step"],
          "selectors": ["step"],
          "match": {
            "step=0": "crypto assets, real world assets, and derivatives in Dango's blazingly fast exchange. Enjoy deep liquidity, fast execution, and fair prices.",
            "step=1": "with Dango's unified trading account with low cost and high capital efficiency. Spot, perps, vaults, one account, under one unified margin system.",
            "step=2": " passive yields on your idle assets by depositing in Dango's vaults. Provide liquidity to facilitate trades, or participate in complex strategies operated by professional traders."
          }
        }
      ]
    },
    "stepper": {
      "steps": [
        {
          "declarations": ["input step"],
          "selectors": ["step"],
          "match": {
            "step=0": "Credential",
            "step=1": "Username"
          }
        }
      ],
      "title": [
        {
          "declarations": ["input step"],
          "selectors": ["step"],
          "match": {
            "step=0": "Create Your Account",
            "step=1": "Select a Username"
          }
        }
      ],
      "description": [
        {
          "declarations": ["input step"],
          "selectors": ["step"],
          "match": {
            "step=0": "Choose a sign in credential. You can add or remove credentials afterwards.",
            "step=1": "Your username will be public on-chain and cannot be changed afterwards."
          }
        }
      ]
    },
