{
  "name": "@leftcurve/types",
  "version": "0.1.0",
  "private": true,
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "types": "./build/index.d.ts",
  "typings": "./build/index.d.ts",
  "sideEffects": false,
  "files": ["build", "src", "!**/*.spec.ts", "!typedoc.json", "!tsup.config.ts"],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "default": "./build/index.js"
    },
    "./window": {
      "types": "./build/window.d.ts",
      "import": "./build/window.mjs",
      "default": "./build/window.js"
    }
  },
  "typesVersions": {
    "*": {
      "window": ["./build/window.d.ts"]
    }
  },
  "scripts": {
    "ci": "pnpm lint",
    "clean": "rm -rf ./build && rm -rf node_modules",
    "clean:build": "rm -rf ./build",
    "lint": "pnpm biome check --write src",
    "lint:fix": "pnpm biome ci ./src",
    "pre-dev": "pnpm build",
    "dev": "pnpm tsup --watch --onSuccess 'tsup --dts-only --silent' --silent",
    "build": "pnpm tsup --clean"
  },
  "devDependencies": {
    "@leftcurve/config": "workspace:^"
  }
}
